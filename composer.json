{
    "name": "geodeticca/spatial",
    "description": "Simple package for working with GDAL library.",
    "keywords": ["gdal"],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "Geodeticca\\Spatial\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
